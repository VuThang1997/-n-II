<Window x:Class="Test6.FunnyList"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Test6"
        mc:Ignorable="d"
        Title="FunnyList" Height="300" Width="510">
    <Grid>
        <ListBox Margin="11,13,0,11" Name="LeftListBox" HorizontalAlignment="Left" Width="163"/>
        <ListBox Margin="0,13,21,11" Name="RightListBox" HorizontalAlignment="Right" Width="158" />

        <Button Name="AddButton" Height="23" Margin="199,13,240,0" VerticalAlignment="Top"
            Click="AddButton_Click" RenderTransformOrigin="0.65,1.087">Add &gt;&gt;</Button>

        <Button Name="RemoveButton" Margin="199,67,220,0"
            Click="RemoveButton_Click" RenderTransformOrigin="0.651,0.348" Height="23" VerticalAlignment="Top">&lt;&lt; Remove</Button>
    </Grid>
</Window>
